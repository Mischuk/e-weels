#nav-service
  position relative
  background #fff url("../images/screen_5/bg.png") no-repeat top center
  margin-top -130px
  padding-top 130px
  padding-bottom 171px
  z-index 1
  .title
    heavy()
    margin-bottom 40px
    padding-top 45px
    font-size 48px
    text-transform uppercase
    text-align center
    color #fff
  .row
    clearfix()
  .maps
    width 650px
    height 370px
    float left
    margin-left 15px
  #map-canvas
    height 370px
    width 100%
    margin 0px
    padding 0px
  .maps-trigger
    float right
    width 490px
    height 370px
    margin-right 15px
    .item
      width 490px
      height 117px
      padding-top 20px
      padding-left 20px
      margin-bottom 9px
      padding-left 30px
      cursor pointer
      background url("../images/screen_5/normal.png") no-repeat top right
      &.current
        background url("../images/screen_5/current.png") no-repeat top right
        .name
          color $orange
        .locate, .clock
          color #fff
      .name
        heavy()
        font-size 22px
        margin-bottom 8px
        padding-left 20px
      .locate, .clock
        font-size 17px
        padding-left 20px



